{
  "name": "golang-bible-learning",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "predev": "node scripts/copy-content.js",
    "dev": "vite",
    "prebuild": "node scripts/copy-content.js",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "biome lint ./src",
    "lint:fix": "biome check --write ./src",
    "format": "biome format --write ./src",
    "format:check": "biome format ./src",
    "check": "biome check ./src",
    "check:fix": "biome check --write ./src",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run type-check && pnpm run check",
    "crawl": "node scripts/crawler.js"
  },
  "dependencies": {
    "@types/react-syntax-highlighter": "^15.5.13",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.20.0",
    "react-syntax-highlighter": "^15.6.6",
    "remark-gfm": "^4.0.0",
    "turndown": "^7.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
